{
  "extends": [
    "tslint:latest",
    "tslint:recommended",
    "tslint-eslint-rules",
    "tslint-config-airbnb"
  ],
  "rules": {
    "prefer-array-literal": false,
    "variable-name": false,
    "import-name": false,
    "no-unused-expression": [
      "allow-tagged-template"
    ],
    "max-classes-per-file": [false]
  }
}